{
  "name": "maik.h",
  "version": "2.0.0",
  "description": "Fast and light HTML templating without the render loop",
  "browser": "default-maik.min.js",
  "homepage": "https://github.com/kaste/maik.h",
  "main": "default-maik.js",
  "scripts": {
    "build": "rollup -c",
    "postbuild": "npm run minify",
    "buildrx": "rollup src/minimal-rx.js -o minimal-rx.js -f iife -n Rx",
    "postbuildrx": "uglifyjs minimal-rx.js -o minimal-rx.min.js --compress --mangle",
    "dev": "rollup -c -w",
    "minify": "uglifyjs default-maik.js --comments=\"/^!/\" --compress --mangle -o default-maik.min.js",
    "postminify": "gzip -c default-maik.min.js | wc -c",
    "size": "cat default-maik.js | wc -c;cat default-maik.min.js | wc -c;gzip -c default-maik.min.js | wc -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaste/maik.h"
  },
  "keywords": [
    "dom",
    "template",
    "fast",
    "performance",
    "diff"
  ],
  "author": "kaste",
  "license": "ISC",
  "devDependencies": {
    "@reactivex/rxjs": "^5.5.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "jsdom": "^9.12.0",
    "mocha": "^4.0.1",
    "prettier": "^1.7.4",
    "rollup": "0.51",
    "rollup-plugin-root-import": "^0.2.2",
    "rxjs-es": "^5.0.0-beta.12",
    "tressa": "^0.3.1",
    "uglify-es": "^3.1.5"
  },
  "dependencies": {
    "majinbuu": "kaste/majinbuu"
  }
}
